<p id="notice"><%= notice %></p>

<h1>Images Items</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Item</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images_items.each do |images_item| %>
      <tr>
        <td><%= images_item.image %></td>
        <td><%= images_item.item %></td>
        <td><%= link_to 'Show', images_item %></td>
        <td><%= link_to 'Edit', edit_images_item_path(images_item) %></td>
        <td><%= link_to 'Destroy', images_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Images Item', new_images_item_path %>
